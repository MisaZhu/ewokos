	cd gterminal;make clean
	cd textgrid;make clean
	cd display;make clean
	cd libpng; make clean
	cd freetype; make clean
	cd font;make clean
	cd minimp3;make clean
	cd graph;make clean
	cd fb;make clean
	cd keyb;make clean
	cd mouse;make clean
	cd fbd;make clean
	cd c++/stl;make clean
	cd c++/c++;make clean

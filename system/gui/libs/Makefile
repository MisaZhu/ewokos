	cd mouse;make
	cd libpng; make
	cd freetype; make
	cd graph;make
	cd font;make
	cd fb;make
	cd display;make
	cd fbd;make
	cd textgrid;make
	cd gterminal;make
	cd c++/stl;make
	cd c++/c++;make
	cd c++/object++;make

clean:
